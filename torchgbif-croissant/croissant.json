  },
  "@type": "sc:Dataset",
  "distribution": [
    {
      "@type": "cr:FileObject",
      "@id": "repo",
      "name": "repo",
      "description": "The Hugging Face git repository.",
      "contentUrl": "https://huggingface.co/datasets/thisistaimur/torchgbif-batches-sample/tree/refs%2Fconvert%2Fparquet",
      "encodingFormat": "git+https",
      "sha256": "https://github.com/mlcommons/croissant/issues/80"
    },
    {
      "@type": "cr:FileSet",
      "@id": "parquet-files-for-config-default",
      "containedIn": {
        "@id": "repo"
      },
      "encodingFormat": "application/x-parquet",
      "includes": "default/*/*.parquet"
    }
  ],
  "recordSet": [
    {
      "@type": "cr:RecordSet",
      "dataType": "cr:Split",
      "key": {
        "@id": "default_splits/split_name"
      },
      "@id": "default_splits",
      "name": "default_splits",
      "description": "Splits for the default config.",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "default_splits/split_name",
          "dataType": "sc:Text"
        }
      ],
      "data": [
        {
          "default_splits/split_name": "train"
        }
      ]
    },
    {
      "@type": "cr:RecordSet",
      "@id": "default",
      "description": "thisistaimur/torchgbif-batches-sample - 'default' subset\n\nAdditional information:\n- 1 skipped column: target_name",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "default/split",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "fileProperty": "fullpath"
            },
            "transform": {
              "regex": "default/(?:partial-)?(train)/.+parquet$"
            }
          },
          "references": {
            "field": {
              "@id": "default_splits/split_name"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "default/num_batches",
          "dataType": "cr:Int64",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "num_batches"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "default/batch_size",
          "dataType": "cr:Int64",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "batch_size"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "default/total_samples",
          "dataType": "cr:Int64",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "total_samples"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "default/dataset_type",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "dataset_type"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "default/saved_files",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "saved_files"
            }
          },
          "isArray": true,
          "arrayShape": "-1"
        },
        {
          "@type": "cr:Field",
          "@id": "default/created_at",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "created_at"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "default/created_by",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "created_by"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "default/dataset_name",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "dataset_name"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "default/dataset_description",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "dataset_description"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "default/feature_names",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "feature_names"
            }
          },
          "isArray": true,
          "arrayShape": "-1"
        },
        {
          "@type": "cr:Field",
          "@id": "default/gbif_download_key",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "gbif_download_key"
            }
          }
        }
      ]
    }
  ],
  "conformsTo": "http://mlcommons.org/croissant/1.1",
  "name": "torchgbif-batches-sample",
  "description": "thisistaimur/torchgbif-batches-sample dataset hosted on Hugging Face and contributed by the HF Datasets community",
